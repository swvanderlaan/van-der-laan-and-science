@ARTICLE{Van_der_Laan2016-zv,
  title    = "Cystatin {C} and cardiovascular disease: A Mendelian
              randomization study",
  author   = "van der Laan, Sander W and Fall, Tove and Soumar{\'e}, Aicha and
              Teumer, Alexander and Sedaghat, Sanaz and Baumert, Jens and
              Zabaneh, Delilah and van Setten, Jessica and Isgum, Ivana and
